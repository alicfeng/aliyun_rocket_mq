{
  "name": "alicfeng/aliyun_rocket_mq",
  "license": "MIT",
  "authors": [
    {
      "name": "alicfeng",
      "email": "a@samego.com"
    }
  ],
  "description": "基于阿里云官方SDK增强组件，更加优雅的应用姿势、更加灵活的动态配置，让应用层服务组件更加标准规范",
  "keywords": [
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^7.0|^8.0",
    "ext-json": "*",
    "aliyunmq/mq-http-sdk": "^1.0.3",
    "friendsofphp/php-cs-fixer": "^2.18"
  },
  "require-dev": {
    "mockery/mockery": "^1.4.2",
    "nunomaduro/collision": "^5.0",
    "phpunit/phpunit": "^9.3.3"
  },
  "autoload": {
    "psr-4": {
      "Samego\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "php-cs": [
      "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix src",
      "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix config",
      "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix tests"
    ]
  },
  "bin": [
    "bin/fix_official_pkg.sh"
  ]
}
